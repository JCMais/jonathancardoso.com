    "gatsby-plugin-preload-fonts": "^1.2.7",
    "gatsby-plugin-react-helmet": "^3.2.1",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-plugin-styled-components": "^3.2.1",
    "gatsby-plugin-typescript": "^2.3.1",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-remark-code-titles": "^1.1.0",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-embed-snippet": "^4.3.0",
    "gatsby-remark-images": "^3.2.1",
    "gatsby-remark-prismjs": "^3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-sharp": "^2.4.2",
    "i18next": "^19.4.4",
    "i18next-browser-languagedetector": "^4.2.0",
    "i18next-sync-fs-backend": "^1.1.1",
    "i18next-xhr-backend": "^3.2.2",
    "moment": "2.26.0",
    "prism-react-renderer": "^1.1.0",
    "prismjs": "^1.19.0",
    "react": "^16.13.1",
    "react-dom": "npm:@hot-loader/react-dom",
    "react-feather": "^2.0.3",
    "react-helmet": "^5.2.1",
    "react-i18next": "^11.3.4",
    "react-youtube": "^7.9.0",
    "rebass": "^4.0.7",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-autolink-headings": "^3.0.0",
    "rehype-slug": "^3.0.0",
    "rehype-toc": "^3.0.1",
    "remark-captions": "^2.2.0",
    "styled-components": "^5.0.1",
    "styled-normalize": "^8.0.7",
    "styled-system": "^5.1.5",
    "unist-util-find-after": "^3.0.0",
    "unist-util-visit": "^2.0.2"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.14.0",
    "@graphql-codegen/typescript": "1.14.0",
    "@graphql-codegen/typescript-operations": "1.14.0",
    "@types/i18next-browser-languagedetector": "^3.0.0",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/node": "^13.13.4",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "@types/rebass": "^4.0.5",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-custom-alias": "^1.2.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-mdx": "^1.6.9",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "typescript-eslint": "^0.0.1-alpha.0",
    "typescript-styled-plugin": "^0.15.0",
    "write": "^2.0.0"
  },
  "keywords": [
    "gatsby",
    "blog",
    "portfolio"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop --host 0.0.0.0",
    "clean:dev": "yarn clean && yarn dev",
    "dev:watch": "nodemon --verbose --exec yarn dev",
    "clean:dev:watch": "yarn clean && yarn dev:watch",
    "d:w": "yarn dev:watch",
    "c:d:w": "yarn clean:dev:watch",
    "clean": "rimraf .cache public",
    "debug": "nodemon --exec \"node --inspect-brk --no-lazy node_modules/.bin/gatsby develop\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.mdx",
    "gtd": "graphql-typescript-definitions --schema-path 'graphql/schema.graphql' --schema-types-path 'src/schema'",
    "prettier-write": "prettier --write \"src/**/*.js\"",
    "release": "auto shipit",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "type-check": "tsc --pretty --noEmit",
    "serve": "gatsby serve",
    "c:b:s": "yarn clean && gatsby build && gatsby serve",
    "gen:gatsby": "graphql-codegen --config codegen.gatsby.yml",
    "gen:src": "graphql-codegen --config codegen.src.yml",
    "g:g": "yarn gen:gatsby",
    "g:s": "yarn gen:src",
    "g": "concurrently \"yarn gen:gatsby\" \"yarn gen:src\"",
    "preload-fonts": "gatsby-preload-fonts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx|js|jsx|mdx)": [
      "yarn lint --fix",
      "yarn prettier-write"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      ".cache/*",
      "public/*",
      "schema.json",
      "src/*"
    ],
    "delay": "1500"
  }
}
